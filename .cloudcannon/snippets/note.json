{
      "snippet": "<Note title=[[type]]>[[body]]</Note>",
      "inline": false,
      "preview": {
        "text": [   
          {
            "template": "{type}"
          },
          "Note"
        ],
        "subtext": [
          {
            "key": "body"
          }
        ],
        "icon": "note"
      },
      "_inputs": {
        "body": {
          "type": "markdown"
        },
        "type": {
          "type": "select",
          "options": {
            "values": [
              "note",
              "warning"
            ]
          }
        }
      },
      "params": {
        "type": {
          "parser": "argument",
          "options": {
            "model": 
            {
                "editor_key": "type",
                "type": "string"
            },
            "format": {
              "ref": "mdx_format"
            },
            "style": { 
                "output": "inline"
            }
          }
        },
        "body": {
          "parser": "content",
          "options": {
            "editor_key": "body",
            "allow_nested": false
          }
        }
      }
}